<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="klipper_api.listener.image_path_upload_subscriber"
                 class="Klipper\Bundle\ApiBundle\Listener\ImagePathUploadSubscriber"
                 public="false">
            <tag name="kernel.event_subscriber" />
            <argument type="service" id="klipper_resource.domain_manager" />
            <argument type="service" id="klipper_content.image_manipulator.cache" on-invalid="null" />
            <argument type="service" id="filesystem" on-invalid="null" />
        </service>
    </services>
</container>
